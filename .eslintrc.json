{
    "root": true,
    "extends": "airbnb-base/legacy",
    "globals": {
        "customer": true,
        "session": true,
        "empty": true,
        "request": true,
        "response": true
    },
    "rules": {
        "import/no-unresolved": "off",
        "indent": ["error", 4, { "SwitchCase": 1, "VariableDeclarator": 0 }],
        "func-names": 0,
        "require-jsdoc": "off",
        "valid-jsdoc": "off",
        "vars-on-top": "off",
        "global-require": "off",
        "no-shadow": ["error", { "allow": ["err", "callback"]}],
        "max-len": "off",
        "linebreak-style": 0
    }
}