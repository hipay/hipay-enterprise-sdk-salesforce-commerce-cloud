    <payment-card card-type="Maestro">
        <name xml:lang="x-default">Maestro</name>
        <enabled-flag>true</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>9-16</length>
            </number-length>
            <number-prefix>
                <prefix>62-67</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>3</security-code-length>
        <custom-attributes>
            <custom-attribute attribute-id="hipayProductName">maestro</custom-attribute>
        </custom-attributes>
    </payment-card>

